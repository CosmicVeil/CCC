#include <bits/stdc++.h>
using namespace std;

int main(int argc, const char * argv[]) {
    // insert code here...
    int N;
    cin >> N;
    
    vector<int> readings;
    readings.resize(N);
    vector<int> most_freq;
    int max_freq = 0;
    int max2_freq = 0;
    vector<int> most2_freq;
    
    vector<int> freq;
    freq.resize(2003);
    
    for(int j = 0; j < 2003;j++){
        freq[j] = 0;
    }
    
    for(int i = 0; i < N;i++){
        
        cin >> readings[i];
        
        freq[readings[i]]++;
    }
    
    for(int i = 1; i < 2003;i++){
        

        int curr_freq = freq[i];
        
        if(curr_freq == 0){
            continue;
        }
        
        if(curr_freq > max_freq){
            
            if(max_freq == max2_freq){
                for(int i = 0; i < most_freq.size();i++){
                    most2_freq.push_back(most_freq[i]);
                }
            }else{
                most2_freq.clear();
                for(int i = 0; i < most_freq.size();i++){
                    most2_freq.push_back(most_freq[i]);
                }
                max2_freq = max_freq;
            }
            most_freq.clear();
            most_freq.push_back(i);
            max_freq = curr_freq;
            
        }else if(curr_freq == max_freq){
            most_freq.push_back(i);
        }else if(curr_freq > max2_freq){
            most2_freq.clear();
            most2_freq.push_back(i);
            max2_freq = curr_freq;
        }else if(curr_freq == max2_freq){
            most2_freq.push_back(i);
        }
    }
    
    if(most2_freq.size() == 0){
        most2_freq = most_freq;
    }
    
    if(most_freq.size() > 1){
        cout << abs(*max_element(most_freq.begin(), most_freq.end())-*min_element(most_freq.begin(), most_freq.end()));
    }else{
        
        int val1 = abs(*max_element(most_freq.begin(), most_freq.end()) - *min_element(most2_freq.begin(), most2_freq.end()));
        int val2 = abs(*max_element(most2_freq.begin(), most2_freq.end()) - *min_element(most_freq.begin(), most_freq.end()));
        
        cout << max(val1,val2) << endl;
    }
    return 0;
}
